@using ProyectoAccenture.Models
@model Editoriales


@{
    ViewBag.Title = "EditarEditorial";
}


<main class="container">

    <form method="post">

        Nombre:
        <input type="text" name="Nombre" value="@this.Model.Nombre" class="form-control" required maxlength="20" placeholder="Ingrese un nombre..." />
        @Html.HiddenFor(model => this.Model.id_editorial)


        <button class="btn btn-primary form-control" style="margin-top:1em">
            Enviar
        </button>

    </form>




</main>


